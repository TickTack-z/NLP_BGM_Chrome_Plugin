{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [ "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  , "default_popup": "popup.html"
  }
  ,"permissions": ["tabs"]
}
